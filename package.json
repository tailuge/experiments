{
    "name": "experiments",
    "description": "Experiments",
    "version": "0.1.0",
    "author": "tailuge",
    "main": "dist/main.js",
    "license": "GPL-3.0",
    "engines": {
        "yarn": "1.x"
    },
    "scripts": {
        "test": "jest --verbose",
        "coverage": "jest --coverage --coverageReporters json",
        "dev": "webpack ",
        "prettify": "prettier --write --no-semi ./dist/*.html main.js",
        "serve": "webpack serve --client-web-socket-url $(gp url 8080 | sed 's/http/ws/' | sed 's#$#/ws#')",
        "deps": "npx yarn-check --skip-unused",
        "fetch": "./fetch.sh",
        "shellcheck": "shellcheck *.sh",
        "train": "python train.py"
    },
    "dependencies": {
        "@tensorflow/tfjs": "^4.9.0",
        "onnxruntime-web": "^1.15.1"
    },
    "devDependencies": {
        "copy-webpack-plugin": "^11.0.0",
        "node-notifier": "^10.0.1",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    }
}
